{
  "name": "passive-buzzer",
  "version": "1.0.0",
  "author": "ArthurZheng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/openblock-plugin/passiveBuzzer.git"
  },
  "type": "module",
  "scripts": {
    "dev": "npx openblock-registry-cli dev",
    "build": "npx openblock-registry-cli build",
    "build:obfuscate": "npx openblock-registry-cli build --obfuscate",
    "publish": "npx openblock-registry-cli publish",
    "publish:dry": "npx openblock-registry-cli publish --dry-run",
    "validate": "npx openblock-registry-cli validate",
    "i18n:update": "npx openblock-registry-cli i18n update",
    "lint": "eslint . --ext .js --fix"
  },
  "openblock": {
    "name": {
      "formatMessage": {
        "id": "passiveBuzzer.name",
        "default": "Passive Buzzer"
      }
    },
    "extensionId": "passiveBuzzer",
    "supportDevice": [
        "arduinoUno",
        "arduinoNano",
        "arduinoLeonardo",
        "arduinoMega2560",
        "arduinoEsp8266"
    ],
    "iconURL": "./assets/passiveBuzzer.png",
    "description": {
      "formatMessage": {
        "id": "passiveBuzzer.description",
        "default": "Unlike the active buzzer, the passive buzzer can be used to make different tones of sound and play some ringtones."
      }
    },
    "main": "./src/passiveBuzzer.js",
    "generator": "./src/generator.js",
    "translations": "./src/translations.js",
    "libraries": "./libraries",
    "tags": ["actuator", "audio"],
    "helpLink": "https://wiki.openblock.cc"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.3.0",
    "eslint-config-scratch": "^5.0.0",
    "husky": "^9.0.0"
  }
}
